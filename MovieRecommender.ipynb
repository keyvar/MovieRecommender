    "class AEncoder(nn.Module):\n",
    "    def __init__(self,):\n",
    "        super(AEncoder, self).__init__()\n",
    "        self.dense1 = nn.Linear(num_movies, 20)\n",
    "        self.dense2 = nn.Linear(20, 10)\n",
    "        self.dense3 = nn.Linear(10, 20)\n",
    "        self.dense4 = nn.Linear(20, num_movies)\n",
    "        self.activation = nn.Sigmoid()\n",
    "    def forward(self, X):\n",
    "        X = self.activation(self.dense1(X))\n",
    "        X = self.activation(self.dense2(X))\n",
    "        X = self.activation(self.dense3(X))\n",
    "        X = self.dense4(X)\n",
    "        return X\n",
    "        "
   ]
