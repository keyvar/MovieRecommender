   "source": [
    "num_epochs = 200\n",
    "for epoch in range(num_epochs):\n",
    "    train_loss = 0.\n",
    "    s = 0.\n",
    "    for uid in range(num_users):       \n",
    "        input = Variable(X_torch[uid]).unsqueeze(0)\n",
    "        target = input.clone()\n",
    "        if torch.sum(target.data > 0) > 0:         \n",
    "            output = aencoder(input)\n",
    "            target.require_grad = False\n",
    "            output[target == 0] = 0\n",
    "            loss = criterion(output, target)\n",
    "            mean_correction = num_movies / float(torch.sum(target.data > 0) + 1e-10)\n",
    "            loss.backward()\n",
    "            train_loss += np.sqrt(loss.data*mean_correction)\n",
    "            s += 1.\n",
    "            optimizer.step()\n",
    "    print('epoch', epoch, 'loss', train_loss / s)\n",
    "    "
   ]
